<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.textviewdemo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppBaseTheme" >
        <activity
            android:name="com.lianpos.devfoucs.login.activity.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.lianpos.devfoucs.homepage.activity.MakeActivity" />
        <activity
            android:name="com.lianpos.activity.MainActivity"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name="com.lianpos.devfoucs.login.activity.RegisterActivity" />
        <activity android:name="com.lianpos.devfoucs.login.activity.RegisterInfoActivity" />
        <activity android:name="com.lianpos.devfoucs.login.activity.RegisterAreaActivity" />
    </application>

</manifest>